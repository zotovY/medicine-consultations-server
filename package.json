{
    "name": "server",
    "version": "0.0.1",
    "description": "web server for medicine consultation app",
    "main": "server.ts",
    "scripts": {
        "start": "set MODE=production&&node ./build/index.js",
        "dev": "export MODE=dev&&ts-node-dev --respawn ./index.ts",
        "build": "tsc",
        "test": "echo 123",
        "fake": "set MODE=fake&&ts-node ./index.ts",
        "isolate": "ts-node-dev --respawn test.ts",
        "postinstall": "npm run build",
        "sandbox": "set MODE=dev&&ts-node-dev --respawn ./sandbox.ts"
    },
    "keywords": [
        "express"
    ],
    "author": "panda.code",
    "license": "ISC",
    "dependencies": {
        "@sentry/node": "^5.20.1",
        "@types/express-fileupload": "^1.1.5",
        "@types/socket.io": "^2.1.11",
        "ajv": "^6.12.5",
        "ajv-keywords": "^4.0.0",
        "bcrypt": "^5.0.0",
        "colors": "^1.4.0",
        "connect": "^3.7.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "ejs": "^3.1.6",
        "express": "^4.17.1",
        "express-fileupload": "^1.2.0",
        "express-rate-limit": "^5.1.3",
        "joi": "^17.3.0",
        "jsonwebtoken": "^9.0.0",
        "moment": "^2.27.0",
        "mongoose": "^5.9.22",
        "mongoose-extend-schema": "^1.0.0",
        "multer": "^1.4.2",
        "multer-sharp": "^0.6.2",
        "nodemailer": "^6.4.10",
        "path": "^0.12.7",
        "peer": "^0.5.3",
        "peerjs": "^1.3.1",
        "pino": "^6.4.1",
        "react-protected-route-component": "^0.1.1",
        "socket.io": "^2.3.0",
        "ts-node": "^8.10.2"
    },
    "devDependencies": {
        "@types/ajv": "^1.0.0",
        "@types/bcrypt": "^3.0.0",
        "@types/cors": "^2.8.6",
        "@types/ejs": "^3.0.6",
        "@types/express": "^4.17.7",
        "@types/express-rate-limit": "^5.0.0",
        "@types/jest": "^26.0.4",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/mocha": "^7.0.2",
        "@types/mongoose": "^5.7.29",
        "@types/multer": "^1.4.3",
        "@types/node": "^14.0.26",
        "@types/nodemailer": "^6.4.0",
        "@types/supertest": "^2.0.10",
        "coveralls": "^3.1.0",
        "jest": "^26.1.0",
        "nodemon": "^2.0.4",
        "supertest": "^4.0.2",
        "ts-jest": "^26.1.1",
        "ts-node": "^8.10.2",
        "ts-node-dev": "^1.0.0-pre.51",
        "typescript": "^3.9.6"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm start",
        "ext": "ts"
    },
    "jest": {
        "globals": {
            "ts-jest": {
                "tsConfigFile": "tsconfig.json"
            }
        },
        "transform": {
            "\\.ts$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ]
    },
    "globals": {
        "ts-jest": {
            "diagnostics": false
        }
    }
}
